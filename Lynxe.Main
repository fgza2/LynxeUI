			-- offset scaling
			local sizeChangeConnections = {}

			local function updateCanvasSize()

				local page = tab["53"]
				local totalHeight = 0

				for _, child in pairs(page:GetChildren()) do
					if child:IsA("Frame") and child.Name ~= "UIListLayout" and child.Name ~= "UIPadding" then
						local childSize = child.AbsoluteSize

						if childSize.Y > 0 then
							totalHeight = totalHeight + childSize.Y
						else
							totalHeight = totalHeight + child.Size.Y.Offset
						end
						totalHeight = totalHeight + 30
					end
				end

				totalHeight = totalHeight + 30
				page.CanvasSize = UDim2.new(0, 0, 0, totalHeight)
			end

			local function trackChildSizeChanges(child)
				if child:IsA("Frame") and child.Name ~= "UIListLayout" and child.Name ~= "UIPadding" then
					if sizeChangeConnections[child] then
						sizeChangeConnections[child]:Disconnect()
					end

					sizeChangeConnections[child] = child:GetPropertyChangedSignal("AbsoluteSize"):Connect(function()
						task.wait(0.1)
						updateCanvasSize()
					end)
				end
			end

			task.wait(0.1)
			updateCanvasSize()

			for _, child in pairs(tab["53"]:GetChildren()) do
				trackChildSizeChanges(child)
			end

			NewConnection(section["5a"], "ChildAdded", function()
				task.wait(0.1)
				updateCanvasSize()
			end)

			NewConnection(section["5a"], "ChildRemoved", function()
				task.wait(0.1)
				updateCanvasSize()
			end)

			NewConnection(tab["53"], "ChildAdded", function(child)
				if child:IsA("Frame") and child.Name ~= "UIListLayout" and child.Name ~= "UIPadding" then
					task.wait(0.1)
					updateCanvasSize()
					trackChildSizeChanges(child)
				end
			end)

			NewConnection(tab["53"], "ChildRemoved", function(child)
				task.wait(0.1)
				updateCanvasSize()

				if sizeChangeConnections[child] then
					sizeChangeConnections[child]:Disconnect()
					sizeChangeConnections[child] = nil
				end
			end)


