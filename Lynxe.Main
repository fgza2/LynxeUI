		local dragStartPos, frameStartPos = nil, nil
		tree.isDragging = false
		tree.canDrag = true 

		function tree:SetDraggable(enabled)
			tree.canDrag = enabled
			if not enabled then
				tree.isDragging = false
			end
		end

		function tree:IsDraggable()
			return tree.canDrag
		end

		NewConnection(frame, "InputBegan", function(input)
			if not tree.canDrag then return end

			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				tree.isDragging = true
				dragStartPos = input.Position
				frameStartPos = frame.Position
			end
		end)

		NewConnection(UserInputService, "InputChanged", function(input)
			if not tree.canDrag or not tree.isDragging then return end

			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				local delta = input.Position - dragStartPos
				local targetPos = UDim2.new(
					frameStartPos.X.Scale, 
					frameStartPos.X.Offset + delta.X, 
					frameStartPos.Y.Scale, 
					frameStartPos.Y.Offset + delta.Y
				)

				TweenService:Create(frame, TweenInfo.new(0.16), {Position = targetPos}):Play()
			end
		end)

		NewConnection(UserInputService, "InputEnded", function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
