			library.UserData:GetSaveFolder(options.ConfigSaving.FolderName)
			local Data = library.UserData:Load(options.ConfigSaving.FolderName, options.ConfigSaving.FileName)

			if Data then
				tree.ConfigData = Data

				for flag, value in pairs(tree.ConfigData) do
					if tree.Elements[flag] then

						local element = tree.Elements[flag]

						if element.set then
							element:set(value)
						elseif element.SetValue then
							element:SetValue(value)
						elseif element.activated then 
							
							local index = table.find(options["Options"], value)

							if index then
								options["CurrentOption"][1] = value
								element["88"].Text = value
							end
							
						elseif element["c8"] then
							element["c8"].Text = value
							options["Keybind"] = value
						end
					end

					-- Load settings
					if tree.ConfigData["UItoggleBind"] then
						options["UItoggleBind"] = tree.ConfigData["UItoggleBind"]
						tree["3f"].Text = tree.ConfigData["UItoggleBind"]
					end

					if tree.ConfigData["ClockMode"] ~= nil then
