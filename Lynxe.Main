				-- methods, functions
				function toggle:set(val)
					if val == nil then
						toggle.Value = not toggle.Value
					else
						toggle.Value = val
					end

					if toggle.Value then
						TweenService:Create(toggle["6d"], TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(200, 200, 200), Position = UDim2.fromOffset(20, 2)}):Play()
						TweenService:Create(toggle["6c"], TweenInfo.new(0.25), {Color = Color3.fromRGB(150, 150, 150)}):Play()
					else
						TweenService:Create(toggle["6d"], TweenInfo.new(0.25), {BackgroundColor3 = Color3.fromRGB(100, 100, 100), Position = UDim2.fromOffset(2, 2)}):Play()
						TweenService:Create(toggle["6c"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100)}):Play()
					end

					options.Callback(toggle.Value)

					if options["Flag"] then
						tree.ConfigData[options["Flag"]] = toggle.Value
						library.UserData:Save(library.MainOptions.ConfigSaving.FolderName, library.MainOptions.ConfigSaving.FileName, tree.ConfigData)
					end
				end

				-- Load saved value if exists
				if tree.ConfigData[options["Flag"]] ~= nil then
					options["CurrentValue"] = tree.ConfigData[options["Flag"]]
				end

				toggle:set(options["CurrentValue"])

				if options["Flag"] then
