-- Connections
Connections = {}

function NewConnection(object, event, callback)
	local connection = object[event]:Connect(callback)
	table.insert(Connections, connection)
	return connection
end

function eraseConnections()
	for i, v in pairs(Connections) do
		v:Disconnect()
		v = nil
	end
end


