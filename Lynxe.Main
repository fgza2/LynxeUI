			navigation.Visible = false
			topbar.Visible = false
			Pages.Visible = false

			library:tween(SettingsPage, {BackgroundTransparency = 0.85})

			NewConnection(CloseButton, "MouseEnter", function()
				library:tween(CloseButton, {ImageTransparency = 0.05})
			end)

			NewConnection(CloseButton, "MouseLeave", function()
				library:tween(CloseButton, {ImageTransparency = 0.3})
			end)

			NewConnection(CloseButton, "Activated", function()
				library:tween(SettingsPage, {BackgroundTransparency = 1}, function()
					navigation.Visible = true
					topbar.Visible = true
					Pages.Visible = true

					SettingsPage.Visible = false
				end)
			end)

			if not library.IsMobile then
				NewConnection(HotkeyButton["Button"], "Activated", function()
					if options["UIHotkey"] ~= nil then options["UIHotkey"] = nil end

					HotkeyButton["Key"].Text = "..."
					local KeyBind = KeyBinding()

					if KeyBind then
						HotkeyButton["Key"].Text = KeyBind.Name
						options["UIHotkey"] = KeyBind

						tree.ConfigData["UItoggleBind"] = KeyBind.Name
						library.UserData:Save(options.ConfigSaving.FolderName, options.ConfigSaving.FileName, tree.ConfigData)
					end
				end)
			end

		end)

		local ToggleButton = tree["31"]

		NewConnection(ToggleButton["Button"], "Activated", function()
			if tree["ClockMode"] then
				tree["ClockMode"] = false
				library:tween(ToggleButton["Indicate"]["Frame"], {Position = UDim2.new(0, 2, 0, 2)})
				ToggleButton["Indicate"]["Frame"].BackgroundColor3 = Color3.fromRGB(100, 100, 100)
			elseif not tree["ClockMode"] then
				tree["ClockMode"] = true
				library:tween(ToggleButton["Indicate"]["Frame"], {Position = UDim2.new(0, 26, 0, 2)})
				ToggleButton["Indicate"]["Frame"].BackgroundColor3 = Color3.fromRGB(130, 130, 130)
			end

			tree.ConfigData["ClockMode"] = tree["ClockMode"]
			library.UserData:Save(options.ConfigSaving.FolderName, options.ConfigSaving.FileName, tree.ConfigData)
		end)

		if not library.IsMobile then
			NewConnection(UserInputService, "InputBegan", function(input, gpe)
				if gpe then return end

				local keybind = options["UItoggleBind"]
				local EnumBind = Enum.KeyCode[keybind]

				if input.KeyCode == EnumBind then
					if tree.UIactive then
						library:tween(tree["2"], {BackgroundTransparency = 1}, function()
							tree.UIactive = false
						end)

					elseif not tree.UIactive then
						tree.UIactive = true
						library:tween(tree["2"], {BackgroundTransparency = 0.2})
					end
				end
			end)
		end

