			library:tween(tree["d6"], {BackgroundTransparency = 1})
			library:tween(tree["d7"], {TextTransparency = 1})
			library:tween(tree["d8"], {ImageTransparency = 1})

			local rotateTween = TweenService:Create(tree["d8"], rotateInfo, {Rotation = 360})
			rotateTween:Play()
		end)

		task.wait(1)
