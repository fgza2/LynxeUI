			library:tween(tree["d1"], {Transparency = 0})
			library:tween(tree["d2"], {TextTransparency = 0.15})
			library:tween(tree["d3"], {TextTransparency = 0.15})
			library:tween(tree["d4"], {TextTransparency = 0.3})
			library:tween(tree["d5"], {TextTransparency = 0.15})
			library:tween(tree["d6"], {BackgroundTransparency = 0.5})
			library:tween(tree["d7"], {TextTransparency = 0.3})
			library:tween(tree["d8"], {ImageTransparency = 0.15})

			local rotateTween = TweenService:Create(tree["d8"], rotateInfo, {Rotation = 360})
			rotateTween:Play()
		end)

		task.wait(3)

		task.spawn(function()
			library:tween(tree["cd"], {BackgroundTransparency = 1})
			library:tween(tree["cf"], {ImageTransparency = 1})
			library:tween(tree["d1"], {Transparency = 1})
			library:tween(tree["d2"], {TextTransparency = 1})
