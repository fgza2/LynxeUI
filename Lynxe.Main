			local delta = input.Position - tree.dragStart
			local position = UDim2.new(tree.startPos.X.Scale, tree.startPos.X.Offset + delta.X, tree.startPos.Y.Scale, tree.startPos.Y.Offset + delta.Y)
			game:GetService("TweenService"):Create(UIFrame, TweenInfo.new(tree.dragSpeed), {Position = position}):Play()
		end

		UIFrame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
				tree.dragToggle = true
				tree.dragStart = input.Position
				tree.startPos = UIFrame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						tree.dragToggle = false
					end
				end)
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
