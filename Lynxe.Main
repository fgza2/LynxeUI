				-- methods, functions
				function slider:SetValue(value)
					if value == nil then
						local ps = math.clamp((mouse.X - slider["76"].AbsolutePosition.X) / slider["76"].AbsoluteSize.X, 0, 1)

						local val = math.floor((options.Range.min + (options.Range.max - options.Range.min) * ps) / options.Increment + 0.5) * options.Increment
						val = math.clamp(val, options.Range.min, options.Range.max)

						slider["7d"].Text = string.format("%.1f", val)
						TweenService:Create(slider["79"], TweenInfo.new(0.15), {Size = UDim2.new(ps, 0, 1, 0)}):Play()

					elseif value ~= nil then
						value = math.floor((value - options.Range.min) / options.Increment + 0.5) * options.Increment + options.Range.min
						value = math.clamp(value, options.Range.min, options.Range.max)

						slider["7d"].Text = string.format("%.1f", value)

						local Size = UDim2.fromScale((value - options.Range.min) / (options.Range.max - options.Range.min), 1)
						TweenService:Create(slider["79"], TweenInfo.new(0.25), {Size = Size}):Play()
					end

					options["CurrentValue"] = tonumber(slider["7d"].Text)
					options.Callback(slider:GetValue())

					if options["Flag"] then
						tree.ConfigData[options["Flag"]] = slider:GetValue()
						library.UserData:Save(library.MainOptions.ConfigSaving.FolderName, library.MainOptions.ConfigSaving.FileName, tree.ConfigData)
					end
				end

				function slider:GetValue()
					return tonumber(slider["7d"].Text)
				end

				-- Load saved value if exists
				if tree.ConfigData[options["Flag"]] ~= nil then
					options["CurrentValue"] = tree.ConfigData[options["Flag"]]
				end

				slider:SetValue(options["CurrentValue"])

				if options["Flag"] then
					tree.Elements[options["Flag"]] = slider
					tree.ConfigData[options["Flag"]] = options["CurrentValue"]
				end

				local stc = nil

				local stc = slider["7d"]:GetPropertyChangedSignal("Text"):Connect(function()
					slider:SetValue(tonumber(slider["7d"].Text))
				end)

				table.insert(Connections, stc)

