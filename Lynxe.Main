						local optionCount = #options["Options"]
						local NewSizeY = nil

						if optionCount > 3 then
							NewSizeY = (optionCount * 57) - 15

						elseif optionCount < 3 then
							NewSizeY = (optionCount * 57) + 15

						else
							NewSizeY = (optionCount * 57)
						end
						
						TweenService:Create(dropdown["80"], TweenInfo.new(0.35), {Size = UDim2.new(1, -1, 0, NewSizeY)}):Play()
						TweenService:Create(dropdown["86"], TweenInfo.new(0.35), {ImageColor3 = Color3.fromRGB(230, 230, 230), Rotation = -90}):Play()

						dropdown["8a"].Visible = true
						TweenService:Create(dropdown["8c"], TweenInfo.new(0.25), {Color = Color3.fromRGB(200, 200, 200), Transparency = 0}):Play()
					else
						TweenService:Create(dropdown["80"], TweenInfo.new(0.35), {Size = UDim2.new(1, -1, 0, 30)}):Play()
						TweenService:Create(dropdown["86"], TweenInfo.new(0.35), {ImageColor3 = Color3.fromRGB(170, 170, 170), Rotation = 0}):Play()

						local t = TweenService:Create(dropdown["8c"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100), Transparency = 0.7})
						t:Play()

						NewConnection(t, "Completed", function()
							dropdown["8a"].Visible = false
						end)
					end
				end

