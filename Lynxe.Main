					dropdown[keyPrefix .. "_padding"]["PaddingBottom"] = UDim.new(0, 6);


					NewConnection(dropdown[keyPrefix], "MouseEnter", function()
						dropdownOption.Hover = true
						TweenService:Create(dropdown[keyPrefix .. "_name"], TweenInfo.new(0.25), {TextTransparency = 0.15}):Play()
						TweenService:Create(dropdown[keyPrefix .. "_stroke"], TweenInfo.new(0.25), {Color = Color3.fromRGB(200, 200, 200)}):Play()
					end)

					NewConnection(dropdown[keyPrefix], "MouseLeave", function()
						dropdownOption.Hover = false

						if not dropdown.MouseDown then
							TweenService:Create(dropdown[keyPrefix .. "_name"], TweenInfo.new(0.25), {TextTransparency = 0.5}):Play()
							TweenService:Create(dropdown[keyPrefix .. "_stroke"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100)}):Play()
						end
					end)

					NewConnection(UserInputService, "InputBegan", function(input)
						if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and dropdownOption.Hover then
							dropdownOption.MouseDown = true
							TweenService:Create(dropdown[keyPrefix], TweenInfo.new(0.1), {BackgroundTransparency = 0.95}):Play()

							local index = table.find(options["Options"], name)

							if index then
								options["CurrentOption"][1] = options["Options"][index]
								dropdown["84"].Text = options["Name"] .. ":  " .. options["CurrentOption"][1]
							end

							options.Callback(options["CurrentOption"][1])

							if options["Flag"] then
								tree.ConfigData[options["Flag"]] = options["CurrentOption"][1]
								library.UserData:Save(library.MainOptions.ConfigSaving.FolderName, library.MainOptions.ConfigSaving.FileName, tree.ConfigData)
							end
						end
					end)

					NewConnection(UserInputService, "InputEnded", function(input)
						if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
							dropdownOption.MouseDown = false	

							if dropdownOption.Hover then
								TweenService:Create(dropdown[keyPrefix], TweenInfo.new(0.1), {BackgroundTransparency = 1}):Play()
							else
								TweenService:Create(dropdown[keyPrefix], TweenInfo.new(0.1), {BackgroundTransparency = 1}):Play()
								TweenService:Create(dropdown[keyPrefix .. "_name"], TweenInfo.new(0.25), {TextTransparency = 0.5}):Play()
								TweenService:Create(dropdown[keyPrefix .. "_stroke"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100)}):Play()
							end
						end
					end)
				end

				function dropdown:addOptions()
					for i, String in pairs(options["Options"]) do
						dropdown:CreateOption(String)
					end
				end

				if tree.ConfigData[options["Flag"]] ~= nil then
					options["CurrentOption"][1] = tree.ConfigData[options["Flag"]]
					dropdown["84"].Text = options["Name"] .. ":  " .. options["CurrentOption"][1]
				end

				dropdown:addOptions()

				if options["Flag"] then
					tree.Elements[options["Flag"]] = dropdown
					tree.ConfigData[options["Flag"]] = options["CurrentOption"][1]
				end


				-- logic
				NewConnection(dropdown["80"], "MouseEnter", function()
					dropdown.Hover = true
					TweenService:Create(dropdown["84"], TweenInfo.new(0.25), {TextTransparency = 0.15}):Play()
					TweenService:Create(dropdown["83"], TweenInfo.new(0.25), {Color = Color3.fromRGB(200, 200, 200)}):Play()
				end)

				NewConnection(dropdown["80"], "MouseLeave", function()
					dropdown.Hover = false

					if not dropdown.MouseDown then
						TweenService:Create(dropdown["84"], TweenInfo.new(0.25), {TextTransparency = 0.5}):Play()
						TweenService:Create(dropdown["83"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100)}):Play()
					end
				end)

				NewConnection(UserInputService, "InputBegan", function(input)
					if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and dropdown.Hover then
						dropdown.MouseDown = true
						TweenService:Create(dropdown["80"], TweenInfo.new(0.1), {BackgroundTransparency = 0.95}):Play()
						dropdown:activated()
					end
				end)

				NewConnection(UserInputService, "InputEnded", function(input)
					if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
						dropdown.MouseDown = false	

						if dropdown["80"] and dropdown["84"] and dropdown["83"] then
							if dropdown.Hover then
								TweenService:Create(dropdown["80"], TweenInfo.new(0.1), {BackgroundTransparency = 1}):Play()
							else
								TweenService:Create(dropdown["80"], TweenInfo.new(0.1), {BackgroundTransparency = 1}):Play()
								TweenService:Create(dropdown["84"], TweenInfo.new(0.25), {TextTransparency = 0.5}):Play()
								TweenService:Create(dropdown["83"], TweenInfo.new(0.25), {Color = Color3.fromRGB(100, 100, 100)}):Play()
							end
						end
					end
				end)

				return dropdown
			end

			return section
		end


