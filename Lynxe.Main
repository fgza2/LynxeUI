					tree.CurrentTab = tab
				end
			end

			function tab:Deactivate()
				if tab.Active then
					tab.Active = false
					tab.Hover = false

					library:tween(tab["19"], {TextTransparency = 0.5})
					library:tween(tab["1a"], {ImageTransparency = 0.5})
					library:tween(tab["1c"], {BackgroundTransparency = 1})
					tab["47"].Visible = false
				end
			end

			NewConnection(tab["17"], "MouseEnter", function()
				tab.Hover = true

				if not tab.Active then
					library:tween(tab["19"], {TextTransparency = 0.15})
					library:tween(tab["1a"], {ImageTransparency = 0.15})
				end
			end)

			NewConnection(tab["17"], "MouseLeave", function()
				tab.Hover = false

				if not tab.Active then
					library:tween(tab["19"], {TextTransparency = 0.5})
					library:tween(tab["1a"], {ImageTransparency = 0.5})
				end
			end)

			NewConnection(tab["1b"], "Activated", function()
				if tab.Hover then
					tab:Activate()
				end
			end)

			if tree.CurrentTab == nil then
				tab:Activate()
			end
			
			-- clock update
			task.spawn(function()
				local function getTimeZoneFromOffset(offset)
					local offsets = {
						-- Pacific
						["-12"] = "Baker", ["-11"] = "Samoa", ["-10"] = "HST", ["-9.5"] = "MIT",
						["-9"] = "AKST", ["-8"] = "PST", ["-7"] = "MST",
						
						-- Americas
						["-6"] = "CST", ["-5"] = "EST", ["-4"] = "AST", ["-3.5"] = "NST",
						["-3"] = "BRT", ["-2"] = "FNT", ["-1"] = "AZOT",
						
						-- Europe/Africa
						["0"] = "GMT", ["1"] = "CET", ["2"] = "EET",
						["3"] = "MSK", ["3.5"] = "IRST", ["4"] = "GST",
						
						-- Asia
						["4.5"] = "AFT", ["5"] = "PKT", ["5.5"] = "IST", ["5.75"] = "NPT",
						["6"] = "BST", ["6.5"] = "MMT", ["7"] = "ICT", ["8"] = "CST",
						["8.5"] = "ACWST", ["9"] = "JST", ["9.5"] = "ACST",
						
						-- Australia/Pacific
						["10"] = "AEST", ["10.5"] = "AEDT", ["11"] = "AEDT", ["12"] = "NZST",
						["12.75"] = "CHAST", ["13"] = "NZDT", ["14"] = "LINT"
					}
					return offsets[tostring(offset)] or "UTC" .. (offset >= 0 and "+" or "") .. offset
				end
				
				-- converting 24-hour to 12-hour formatting
				local function formatTime(hour24, minute, second, is12Hour)
					if not is12Hour then
						return string.format("%02d:%02d:%02d", hour24, minute, second)
					end
					
					local period = "AM"
					local hour12 = hour24
					
					if hour24 >= 12 then
						period = "PM"
						if hour24 > 12 then
							hour12 = hour24 - 12
						end
					elseif hour24 == 0 then
						hour12 = 12
					end
					
					return string.format("%d:%02d:%02d %s", hour12, minute, second, period)
				end
				
				while true do
					local currentTime = DateTime.now()
					local timeString = currentTime:FormatLocalTime("HH:mm:ss", "en-us")
					
					local utcTime = currentTime:FormatUniversalTime("HH:mm:ss", "en-us")
					local localHour = tonumber(string.sub(timeString, 1, 2))
					local localMinute = tonumber(string.sub(timeString, 4, 5))
					local localSecond = tonumber(string.sub(timeString, 7, 8))
					
					local utcHour = tonumber(string.sub(utcTime, 1, 2))
					local offset = localHour - utcHour
					
					if offset > 12 then offset = offset - 24 end
					if offset < -12 then offset = offset + 24 end
					
					local timeZoneAbbrev = getTimeZoneFromOffset(offset)
					local formattedTime = formatTime(localHour, localMinute, localSecond, tree.ClockMode)
					
					tab["52"]["Text"] = [[Time: ]] .. formattedTime .. " (" .. timeZoneAbbrev .. ")"
					
					task.wait(1)
				end
			end)
		end
		return tab
	end
	
	
